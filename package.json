{
  "name": "@feed-org/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "node prepend-name.js nx run feed-org:dev",
    "vercel-build": "node prepend-name.js nx run feed-org:build --skip-nx-cache",
    "docker:start": "docker-compose -f infrastructure/docker-compose.postgres.yml up -d",
    "docker:stop": "docker-compose -f infrastructure/docker-compose.postgres.yml down",
    "prisma:migrate": "node prepend-name.js npx prisma migrate dev --schema=libs/data-access/src/lib/prisma/schema.prisma",
    "prisma:generate": "node prepend-name.js npx prisma generate --schema=libs/data-access/src/lib/prisma/schema.prisma",
    "prisma:migrate:create-only": "node prepend-name.js npx prisma migrate dev --create-only --schema=libs/data-access/src/lib/schema.prisma",
    "prisma": "npm run prisma:migrate && npm run prisma:generate"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.16.1",
    "@prisma/client": "^5.16.2",
    "next": "14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-is": "18.3.1",
    "styled-components": "5.3.6",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nx/devkit": "19.4.3",
    "@nx/eslint": "19.4.3",
    "@nx/eslint-plugin": "19.4.3",
    "@nx/jest": "19.4.3",
    "@nx/js": "19.4.3",
    "@nx/next": "19.4.3",
    "@nx/playwright": "19.4.3",
    "@nx/react": "19.4.3",
    "@nx/workspace": "19.4.3",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/plugin-styled-components": "^1.5.67",
    "@types/jest": "^29.4.0",
    "@types/node": "18.16.9",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@types/react-is": "18.3.0",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
